<h1>Sign In</h1>

<%= form_with url: sign_in_path do |form| %>
    <div>
        <%= form.label :email %>
        <%= form.text_field :email %>
    </div>

    <div>
        <%= form.label :password %>
        <%= form.text_field :password %>
    </div>

    <div>
        <%= form.submit "Sign In"%>
    </div>
<% end %>